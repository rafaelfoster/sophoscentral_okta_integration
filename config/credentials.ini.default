# For being able to integrate with SOphos Central 
# You should set your Sophos Central credentials.
# https://docs.sophos.com/central/Customer/help/en-us/central/Customer/tasks/ep_APICredentials.html
#
# Set your credentials and rename this file to credentials.ini 

[sophos_central]
client_id: YOUR_SOPHOS_CENTRAL_CLIENT_ID
client_secret: YOUR_SOPHOS_CENTRAL_CLIENT_SECRET

[okta]
orgUrl: https://YOUR_OKTA_DOMAIN_URL
token: YOUR_OKTA_TOKEN 
